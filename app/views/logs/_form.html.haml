= form_for @log do |f|
  - if @log.errors.any?
    #error_explanation
      %h2= "#{pluralize(@log.errors.count, "error")} prohibited this log from being saved:"
      %ul
        - @log.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :user
    = f.text_field :user
  .field
    = f.label :session_id
    = f.text_field :session_id
  .field
    = f.label :uri
    = f.text_field :uri
  .field
    = f.label :params
    = f.text_area :params
  .actions
    = f.submit 'Save'
